  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jtwebman/node-express-docker-starter/issues"
  },
  "homepage": "https://github.com/jtwebman/node-express-docker-starter#readme",
  "dependencies": {
    "config": "^3.1.0",
    "express": "^4.17.1",
    "stoppable": "^1.1.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
